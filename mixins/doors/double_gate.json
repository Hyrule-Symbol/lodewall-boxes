{
   "type": "mixin",
   "components": {
      "stonehearth:entity_forms": {
         "placeable_on_ground": true
      },
      "sensor_list": {
         "sensors": {
            "door_sensor": {
               "type": "sensor",
               "radius": 2
            }
         }
      },
      "stonehearth:door": {
         "sensor": "door_sensor"
      },
      "region_collision_shape": {
         "region_collision_type": "none",
         "region": [
            {
               "min": { "x": -1.75, "y": 0, "z": 0.25 },
               "max": { "x": 2.75, "y": 2, "z": 0.75 }
            }
         ]
      }
   }
}